{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "ext-json": "*",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "dompdf/dompdf": "^1.1",
        "drupal/admin_toolbar": "^3.0",
        "drupal/ajax_loader": "^2.0",
        "drupal/anonymous_login": "^2.0",
        "drupal/bamboo_twig": "^5.0@alpha",
        "drupal/better_exposed_filters": "^5.0",
        "drupal/blazy": "^2.5",
        "drupal/ckeditor_font": "^1.2",
        "drupal/ckwordcount": "^2.0",
        "drupal/config_ignore": "^2.3",
        "drupal/core-composer-scaffold": "^9.2",
        "drupal/core-project-message": "^9.2",
        "drupal/core-recommended": "^9.2",
        "drupal/diff": "^1.0",
        "drupal/file_delete": "^1.0",
        "drupal/floatpanel": "^1.0",
        "drupal/gin": "^3.0@alpha",
        "drupal/gin_toolbar": "^1.0@beta",
        "drupal/group": "^1.4",
        "drupal/group_taxonomy": "^1.0@beta",
        "drupal/group_term": "^2.0@alpha",
        "drupal/groupmedia": "^2.0@alpha",
        "drupal/image_effects": "^3.1",
        "drupal/login_destination": "^2.0@beta",
        "drupal/mail_login": "^2.4",
        "drupal/media_library_form_element": "^2.0",
        "drupal/menu_admin_per_menu": "^1.3",
        "drupal/menus_attribute": "^1.2",
        "drupal/multivalue_form_element": "^1.0@beta",
        "drupal/panelbutton": "^1.4",
        "drupal/persistent_login": "^1.3",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/redirect_after_login": "^2.7",
        "drupal/search_api": "^1.21",
        "drupal/shield": "^1.4",
        "drupal/status_messages": "^4.2",
        "drupal/svg_image": "^1.15",
        "drupal/swiftmailer": "^2.0",
        "drupal/symfony_mailer": "^1.0@alpha",
        "drupal/taxonomy_multidelete_terms": "^1.3",
        "drupal/transliterate_filenames": "^2.0",
        "drush/drush": "^10.6",
        "jfcherng/php-diff": "^6.11",
        "phpoffice/phpword": "^0.18.2",
        "smalot/pdfparser": "*",
        "spatie/pdf-to-text": "^1.51",
        "subskill/falc": "^1.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true
        }
    },
    "extra": {
        "patches": {
            "drupal/taxonomy_multidelete_terms": {
                "PrivateTempStoreFactory Errors on Drupal 9" : "https://www.drupal.org/files/issues/2020-09-24/PrivateTempStoreFactory-errors-on-drupal9-3172916-2.patch"
            },
            "drupal/group": {
                "Add support for 'view own unpublished entity' permission" : "https://www.drupal.org/files/issues/2021-05-04/2876696-16.patch"
            },
            "drupal/redirect_after_login": {
                "Headers have already been sent after upgrade to Drupal 9.2 (can't login)" : "https://www.drupal.org/files/issues/2021-06-20/3214949.patch"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "/var/www/html/"
            }
        },
        "installer-paths": {
            "/var/www/html/core": [
                "type:drupal-core"
            ],
            "/var/www/html/libraries/{$name}": [
                "type:drupal-library"
            ],
            "/var/www/html/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "/var/www/html/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "/var/www/html/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "/var/www/html/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "/var/www/html/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "/var/www/html/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    },
    "require-dev": {
        "drupal/devel": "^4.1"
    }
}
